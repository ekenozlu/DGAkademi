<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ekenozlu.dgpaysfinal.activities.ui.movies.HomeFragment"
        android:label="@string/title_movies"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.ekenozlu.dgpaysfinal.activities.ui.saved.DashboardFragment"
        android:label="@string/title_saved"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.ekenozlu.dgpaysfinal.activities.ui.profile.NotificationsFragment"
